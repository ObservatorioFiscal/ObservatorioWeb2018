@section Scripts
{
    <script>
        function AsignarSueldo() {
            $("#resultado").text($("#valor").val());
            Consulta();
        }
        function ponerpuntos(numero) {
            var number = numero.toString();
            var result = '';
            while (number.length > 3) {
                result = '.' + number.substr(number.length - 3) + result;
                number = number.substring(0, number.length - 3);
            }
            result = number + result;
            return result;
        }
        function Consulta() {
            $("#resultado").text($("#valor").val());
            $("#divdatos").removeClass("hidden");
            var sueldo = String($("#sueldo").val());
            var sexo = String($("#genero").val());
            var tipo = String($('input[name="inlineRadioOptions"]:checked').val());
            var alcohol = String($("#alcohol").val());
            var bebida = String($("#bebida").val());
            var fuma = String($("#fuma").val());
            var auto = String($("#auto").val());
            $.ajax({
                cache: false,
                async: false,
                type: 'POST',
                data: {
                    sueldo: sueldo,
                    sexo: sexo,
                    tipo: tipo,
                    alcohol: alcohol,
                    bebida: bebida,
                    fuma: fuma,
                    auto: auto
                },
                url: '../../AjaxGraficos/ConsultaApp',
                dataType: 'json',
                success: function (data, textStatus, jqXHR) {
                    $("#resultadofinalsueldo").text("$ " + ponerpuntos(sueldo * 12));
                    $("#resultadofinal").text("$ " + ponerpuntos(data.valor1));
                    $("#resultadofinal2").text("$ " + ponerpuntos(data.valor2));
                    $("#resultadofinal3").text("$ " + ponerpuntos(data.valor3));
