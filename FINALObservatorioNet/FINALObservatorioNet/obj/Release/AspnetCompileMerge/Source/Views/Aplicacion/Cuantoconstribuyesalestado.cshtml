                        var aux = i + 1;
                        $("#datoA" + aux).text("$ " + ponerpuntos(data.valor4[i]));
                    }

                    for (var i = 10; i < 18; i++) {
                        var aux = i - 9;
                        $("#datoA1S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                    for (var i = 18; i < 27; i++) {
                        var aux = i - 17;
