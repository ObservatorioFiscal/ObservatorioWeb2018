                    }
                    for (var i = 27; i < 29; i++) {
                        var aux = i - 26;
                        $("#datoA3S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                    for (var i = 29; i < 32; i++) {
                        var aux = i - 28;
                        $("#datoA4S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                    for (var i = 32; i < 36; i++) {
                        var aux = i - 31;
                        $("#datoA5S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                    for (var i = 36; i < 41; i++) {
                        var aux = i - 35;
                        $("#datoA6S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                    for (var i = 41; i < 47; i++) {
                        var aux = i - 40;
                        $("#datoA7S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                    for (var i = 47; i < 52; i++) {
                        var aux = i - 46;
                        $("#datoA8S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                    for (var i = 52; i < 57; i++) {
                        var aux = i - 51;
                        $("#datoA9S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                    for (var i = 57; i < 59; i++) {
                        var aux = i - 56;
                        $("#datoA10S" + aux).html("$ " + ponerpuntos(data.valor4[i]));
                    }
                },
                error: function () {
                    //alert('error');
                }
            });
        }
        function abrir() {
