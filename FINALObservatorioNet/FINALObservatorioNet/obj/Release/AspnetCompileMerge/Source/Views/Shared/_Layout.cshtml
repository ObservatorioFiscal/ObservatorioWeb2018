        "https://www.facebook.com/observafiscalcl",
        "https://twitter.com/observafiscalcl",
        "https://www.linkedin.com/company/15086872",
        "https://plus.google.com/113548614432745922065"
        ],
        "potentialAction": {
        "@@type": "SearchAction",
        "target": "http://observatoriofiscal.cl/?s={search_term_string}",
        "query-input": "required name=search_term_string"
        }
        }
    </script>
