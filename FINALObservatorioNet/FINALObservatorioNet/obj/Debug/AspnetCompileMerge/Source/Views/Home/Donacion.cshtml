@section Scripts{
    <script>
        $(".dona").click(function () {
            $("#primero").addClass("hidden");
            $("#segundo").removeClass("hidden");
            $("#valor").text($(this).text())

        });

        function volverdona(aux) {
            if (aux == 2) {
                $("#segundo").addClass("hidden");
                $("#primero").removeClass("hidden");
                $("#valor").text("");
            }
            else {
                $("#tercero").addClass("hidden");
                $("#segundo").removeClass("hidden");
            }
        }

        function monto(aux) {
            if ($("#Nombre").val() != "" && $("#email").val() != "") {
                $.ajax({
                    cache: false,
                    async: false,
                    type: 'POST',
                    data: {
                        Nombre: $("#Nombre").val(),
                        Email: $("#email").val(),
                        Monto: $("#valor").val()
                    },
                    url: '../../Home/AgregarPosibleDonacion',
                    dataType: 'json',
                    success: function (data) {
                        if (data == true) {
                            if (aux == 0) {
                                $("#segundo").addClass("hidden");
                                $("#tercero").removeClass("hidden");

                            } else {
                                var aux2 = $("#valor").text();
                                switch (aux2) {
                                    case "$3.000":
                                        $('form#formulario1').submit();
                                        break;
                                    case "$5.000":
                                        $('form#formulario2').submit();
                                        break;
                                    case "$10.000":
                                        $('form#formulario3').submit();
                                        break;
                                    case "$15.000":
                                        $('form#formulario4').submit();
                                        break;
                                    case "$20.000":
                                        $('form#formulario5').submit();
                                        break;
                                    case "$25.000":
                                        $('form#formulario6').submit();
                                        break;
                                    default:
                                        $('form#formulario7').submit();
                                        break;
                                }
                                $('form#formulario').submit();
                            }
                        }
                    }
                });
            }
            else {

            }
            
        }


    </script>


    <script>
        function frases_alea() {

            frases = new Array();

            frases[0] = '<p class="frase text-center">"Aquellos que tienen el privilegio de saber tienen la obligación de actuar."<br /><small>Albert Einstein</small><br /><br /></p>';
            frases[1] = '<p class="frase text-center">"Más ojos que miran el gasto público, mejor será su calidad y eficiencia, y menor el espacio discrecional que facilita la corrupción.."<br /><small>Equipo Observatorio</small><br /><br /></p>';
            
            aleatorio = Math.floor(Math.random() * 2);

            return frases[aleatorio];

        }
        onload = function () {
            document.getElementById('frases').innerHTML = frases_alea();
            setInterval(function () {
                document.getElementById('frases').style.opacity = 0;
                document.getElementById('frases').innerHTML = frases_alea();
                document.getElementById('frases').style.opacity = 1;
            }, 5000)
        }
    </script>
}